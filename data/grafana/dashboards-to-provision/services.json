      "title": "c4dt.org: loading time",
      "type": "graph",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "id": 1,
      "targets": [{
        "refId": "A",
        "target": "c4dt-org.loading-time"
