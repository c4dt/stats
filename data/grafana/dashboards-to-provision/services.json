        "name": "c4dt.org: loading time",
        "conditions": [
          {
            "evaluator": { "params": [ 5 ], "type": "gt" },
            "operator": { "type": "and" },
            "query": { "params": [ "A", "5m", "now" ] },
            "reducer": { "type": "avg" },
            "type": "query"
          }, {
            "evaluator": { "params": [ 4 ], "type": "lt" },
            "operator": { "type": "or" },
            "query": { "params": [ "A", "5m", "now" ] },
            "reducer": { "type": "count_non_null" },
            "type": "query"
          }
        ],
        "frequency": "1m",
        "for": "5m"
      },
      "thresholds": [{
        "colorMode": "critical",
        "op": "gt",
        "value": 5
      }]
